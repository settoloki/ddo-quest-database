{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "lint": "eslint resources/js --ext .js,.jsx --max-warnings 0",
        "lint:fix": "eslint resources/js --ext .js,.jsx --fix",
        "format": "prettier --write \"resources/js/**/*.{js,jsx}\"",
        "format:check": "prettier --check \"resources/js/**/*.{js,jsx}\"",
        "test": "echo \"No frontend tests configured yet\"",
        "type-check": "echo \"TypeScript not configured yet\"",
        "ci": "npm run lint && npm run format:check && npm run build"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.0",
        "@vitejs/plugin-react": "^4.7.0",
        "axios": "^1.8.2",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^2.0.0",
        "tailwindcss": "^4.0.0",
        "vite": "^7.0.4"
    },
    "dependencies": {
        "@chakra-ui/icons": "^2.1.1",
        "@chakra-ui/react": "^2.8.2",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@inertiajs/react": "^2.0.17",
        "framer-motion": "^10.16.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "overrides": {
        "@inertiajs/react": {
            "react": "^18.2.0"
        }
    }
}
